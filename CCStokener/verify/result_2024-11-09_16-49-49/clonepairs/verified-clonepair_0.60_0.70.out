    }
    String response;
    BufferedReader source = new BufferedReader(new InputStreamReader(connection.getInputStream()));
    StringBuffer buf = new StringBuffer();
    while ((response = source.readLine()) != null) {
        buf.append(response);
    }
    source.close();
    return buf.toString();
}



